{
  "name": "gitwit",
  "displayName": "GitWit - AI Code Review",
  "description": "An AI-powered code reviewer with multiple personalities to help you improve your code quality, style, and security.",
  "version": "1.0.0",
  "publisher": "shruti-pandey",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/shrutipandey15/gitwit.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Machine Learning",
    "Programming Languages",
    "Education",
    "Other"
  ],
  "activationEvents": [
    "onCommand:gitwit.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitwit.start",
        "title": "GitWit: Start Review",
        "category": "GitWit"
      }
    ],
    "configuration": {
      "title": "GitWit",
      "properties": {
        "gitwit.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your personal Google AI API key for GitWit to use."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/node-fetch": "^2.6.11",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "node-fetch": "^2.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.0"
  }
}