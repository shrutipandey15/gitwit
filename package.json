{
  "name": "codecritter",
  "displayName": "CodeCritter - AI Code Review",
  "description": "An AI-powered code reviewer with multiple personalities to help you improve your code quality, style, and security.",
  "version": "1.1.1",
  "publisher": "shruti-pandey",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/shrutipandey15/gitwit.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Machine Learning",
    "Programming Languages",
    "Education",
    "Other"
  ],
  "keywords": [
    "ai",
    "code review",
    "gemini",
    "code quality",
    "docstring",
    "security"
  ],
  "activationEvents": [
    "onCommand:codecritter.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codecritter.start",
        "title": "Start Review",
        "category": "CodeCritter"
      }
    ],
    "configuration": {
      "title": "CodeCritter",
      "properties": {
        "codecritter.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your personal Google AI API key for CodeCritter to use. Get one from https://makersuite.google.com/app/apikey"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1"
  }
}